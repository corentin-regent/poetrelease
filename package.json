{
  "name": "poetrel",
  "description": "Github Action for automatic semantic versioning and releases with Poetry",
  "version": "1.4.2",
  "author": "Corentin RÃ©gent",
  "homepage": "https://github.com/corentin-regent/poetrel",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corentin-regent/poetrel.git"
  },
  "bugs": {
    "url": "https://github.com/corentin-regent/poetrel/issues"
  },
  "keywords": [
    "Actions",
    "GitHub",
    "Poetry",
    "Python",
    "Release",
    "Semver"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "format": "npx prettier --write .",
    "package": "bash scripts/package.sh",
    "test": "npx jest --runInBand"
  },
  "license": "MIT",
  "eslintConfig": {
    "extends": "./.eslintrc.yml"
  },
  "jest": {
    "clearMocks": true,
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/preset-env": "^7.24.4",
    "@types/jest": "^29.5.12",
    "@vercel/ncc": "^0.38.1",
    "babel-preset-jest": "^29.6.3",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-jest": "^28.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  }
}
